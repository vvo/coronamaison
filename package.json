{
  "name": "coronomaison",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "NODE_ICU_DATA=node_modules/full-icu next build",
    "dev": "next dev",
    "findDrawings": "babel-node --presets @babel/preset-env findDrawings.js",
    "format": "prettier --write '**/*.*' && eslint . --fix",
    "indexDrawings": "babel-node --presets @babel/preset-env indexDrawings.js",
    "lint": "prettier --check '**/*.*' && eslint . --ext js,jsx",
    "prepareData": "babel-node --presets @babel/preset-env prepareData.js",
    "start": "next start",
    "test": "yarn lint && yarn build",
    "uploadMedia": "babel-node --presets @babel/preset-env uploadMedia.js"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2019,
      "sourceType": "module"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "."
          ],
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      },
      "react": {
        "version": "16.3.0"
      }
    },
    "plugins": [
      "react",
      "react-hooks"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "plugin:import/react"
    ],
    "rules": {
      "curly": "error",
      "import/order": "error",
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react/no-unescaped-entities": "off"
    }
  },
  "dependencies": {
    "@babel/core": "7.9.6",
    "@babel/node": "7.8.7",
    "@babel/preset-env": "7.9.6",
    "@next/bundle-analyzer": "9.4.0",
    "@prettier/plugin-xml": "0.7.2",
    "@svgr/webpack": "5.4.0",
    "@types/luxon": "1.24.0",
    "algoliasearch": "4.2.0",
    "async": "3.2.0",
    "autoprefixer": "9.7.6",
    "babel-eslint": "10.1.0",
    "babel-plugin-inline-react-svg": "1.1.1",
    "eslint": "7.0.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.1",
    "full-icu": "1.3.1",
    "global": "4.4.0",
    "got": "11.1.3",
    "image-size": "0.8.3",
    "imagemin-svgo": "7.1.0",
    "lodash": "4.17.15",
    "lowdb": "1.0.0",
    "luxon": "1.24.1",
    "mkdirp": "1.0.4",
    "next": "9.4.0",
    "next-compose-plugins": "2.2.0",
    "node-ovh-storage": "0.2.0",
    "postcss-import": "12.0.1",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "2.2.3",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-instantsearch-dom": "6.4.0",
    "react-share": "4.1.0",
    "react-tweet-embed": "1.2.2",
    "recursive-readdir": "2.2.2",
    "sharp": "0.25.2",
    "sqip": "1.0.0-alpha.31",
    "sqip-cli": "1.0.0-alpha.35",
    "sqip-plugin-pixels": "1.0.0-alpha.31",
    "sqip-plugin-svgo": "1.0.0-alpha.32",
    "tailwindcss": "1.4.6",
    "vanilla-lazyload": "15.2.0"
  },
  "engines": {
    "node": "12.16.3",
    "yarn": "1.22.4"
  },
  "renovate": {
    "extends": [
      "config:js-app",
      ":automergePatch",
      ":automergeBranch",
      ":automergeLinters",
      ":automergeTesters",
      ":automergeTypes"
    ],
    "lockFileMaintenance": {
      "enabled": true
    }
  }
}
